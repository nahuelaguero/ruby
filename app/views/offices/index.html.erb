<h1>Offices listing page</h1>
<table>
  <th>
    <tr>
      <th>Name</th>
      <th>Organization</th>
      <th colspan="3">Actions</th>
    </tr>
  </th>
  <tbody>
    <% @offices.each do |office| %>
      <tr>
        <td><%= office.name %> &nbsp;</td>
        <td><%= office.organization.name if office.organization_id %> &nbsp;</td>
        <td><%= link_to 'Show', office_path(office) %></td>
        <td><%= link_to 'Edit', edit_office_path(office) %></td>
        <td><%= link_to 'Delete', office_path(office), method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p><%= link_to 'Create new Office', new_office_path %></p>
