<h1>Project#index</h1>

<h1>Projects listing page</h1>
<table>
  <th>
    <tr>
      <th>Name</th>
      <th>Organization</th>
      <th>Status</th>
      <th>People</th>
      <th>Start_date</th>
      <th>End_date</th>
    </tr>
  </th>
  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.organization %></td>
        <td><%= project.status %></td>
        <td><%= project.people %></td>
        <td><%= project.start_date %></td>
        <td>some date</td>
      </tr>
      
    <% end %>
  </tbody>
</table>