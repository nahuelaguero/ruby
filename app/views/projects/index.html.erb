<h1>Projects listing page</h1>
<table>
  <th>
    <tr>
      <th>Name</th>
      <th>Organization</th>
      <th>Status</th>
      <th>Supervisor</th>
      <th>People</th>
      <th>Start date</th>
      <th>End date</th>
      <th colspan="3">Actions</th>
    </tr>
  </th>
  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.organization %></td>
        <td><%= project.status %></td>
        <td><td><%= project.supervisor %></td></td>
        <td><%= project.people %></td>
        <td><%= project.start_date %></td>
        <td><%= project.end_date %></td>
        <td><%= link_to 'Show', project_path(project) %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Delete', project_path(project), method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p><%= link_to 'Create new Project', new_project_path %></p>
