<h1>Organizations listing page</h1>
<table>
  <th>
    <tr>
      <th>Name</th>
      
      <th colspan="3">Actions</th>
    </tr>
  </th>
  <tbody>
    <% @organizations.each do |organization| %>
      <tr>
        <td><%= organization.name %></td>
        <td><%= link_to 'Show', organization_path(organization) %></td>
        <td><%= link_to 'Edit', edit_organization_path(organization) %></td>
        <td><%= link_to 'Delete', organization_path(organization), method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p><%= link_to 'Create new Organization', new_organization_path %></p>
